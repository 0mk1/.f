alias manage="dcomp -f dev.yml run --rm django python3 src/manage.py "
alias mysql56="docker run --name mysql -e MYSQL_ROOT_PASSWORD=admin1234 -p 3306:3306 mysql:5.6"
alias mysql57="docker run --name mysql -e MYSQL_ROOT_PASSWORD=admin1234 -p 3306:3306 mysql:5.7"
alias mysql80="docker run --name mysql -e MYSQL_ROOT_PASSWORD=admin1234 -p 3306:3306 mysql:8.0"
alias postgres10="docker run --name postgres -p 5432:5432 postgres:10.0"
alias postgres96="docker run --name postgres -p 5432:5432 postgres:9.6"
alias serve="dcomp -f dev.yml run --rm --service-ports api "
alias ta="tmux -2 attach -t "
alias tm="tmux -2"
alias v="nvim"
alias venv="mkvirtualenv -p python3 ."
alias vi="nvim"
alias vim="nvim"
alias es-fix-95-storage="curl -XPUT -H "Content-Type: application/json" http://user:password@localhost:9200/*/_settings -d '{"index.blocks.read_only_allow_delete": null}'"
alias tcpdump-docker="tcpdump -l -A -i eth1 port 5000"
alias gpgpass="cat ~/Nextcloud/pass.md"
