alias awsp="_awsSwitchProfile"
alias build="cd . && ddev build && pip install -r requirements.txt"
alias c="cd ~/code/"
alias dcomp="docker-compose "
alias ddev="docker-compose -f dev.yml"
alias g="git"
alias kctx="kubectx "
alias k="kubectl "
alias kens="kubens "
alias kill-mysql="docker rm --force mysql"
alias kill-postgres="docker rm --force postgres"
