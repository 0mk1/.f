alias agi="sudo apt install"
alias agl="sudo dpkg -l | grep "
alias agr="sudo apt purge"
alias ags="sudo apt search"
alias aguu="sudo apt update; sudo apt upgrade;"
alias build="dcomp -f dev.yml build"
alias dcomp="docker-compose "
alias ddev="docker-compose -f dev.yml"
alias diff="git diff"
alias djm="dcomp -f dev.yml run --rm django python3 manage.py "
alias djshell="djm shell_plus"
alias djstarta="djm startapp "
alias djstartp="django-admin startproject "
alias docker-clean-containers='docker rm $(docker ps -qa --no-trunc --filter "status=exited")'
alias docker-clean-images='docker rmi $(docker images --filter "dangling=true" -q --no-trunc)'
alias gall="git add -A"
alias gcb="git checkout -b"
alias gcmt="git commit -v"
alias gco="git checkout"
alias glog="git log --color --graph"
alias mkvnv="mkvirtualenv -p python3 "
alias p="cd ~/Projekty/"
alias pf="pip3 freeze"
alias pull="git pull"
alias push="git push origin"
alias serve="dcomp -f dev.yml run --rm --service-ports django"
alias sshcopy="ssh-copy-id -i ~/.ssh/id_rsa.pub "
alias st="git status --porcelain"
alias ta="tmux -2 attach -t "
alias tm="tmux -2"
alias ts="tmux -2 new -s "
alias v="vim "
alias vnv="workon "
